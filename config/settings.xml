<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      https://maven.apache.org/xsd/settings-1.0.0.xsd">
  <localRepository>/usr/share/maven/ref/repository</localRepository>

  <profiles>
    <!-- added on 2020/09/01, for self-founded maven repo  -->
    <profile>
      <id>bite-private-repo</id>
      <repositories>
        <repository>
          <id>bite</id>
          <url>https://repo.biteinvestments.com/repository/maven-public/</url>
        </repository>
      </repositories>
    </profile>
  </profiles>

  <activeProfiles>
    <activeProfile>bite-private-repo</activeProfile>
  </activeProfiles>
</settings>
